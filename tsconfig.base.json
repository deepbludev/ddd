{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "strict": true,
    "declaration": false,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2022", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@deepblu/ddd": ["libs/packages/ddd/src/index.ts"],
      "@deepblu/ddd-utils": ["libs/packages/ddd-utils/src/index.ts"],
      "@deepblu/examples/transactions-app/contexts/core/tx/modules/transaction/application": [
        "libs/examples/transactions-app/contexts/core/tx/modules/transaction/application/src/index.ts"
      ],
      "@deepblu/examples/transactions-app/contexts/core/tx/shared": [
        "libs/examples/transactions-app/contexts/core/tx/shared/application/src/index.ts"
      ],
      "@deepblu/examples/transactions-app/contexts/core/tx/shared/domain": [
        "libs/examples/transactions-app/contexts/core/tx/shared/domain/src/index.ts"
      ],
      "@deepblu/examples/transactions-app/contexts/core/tx/shared/infra": [
        "libs/examples/transactions-app/contexts/core/tx/shared/infra/src/index.ts"
      ],
      "@deepblu/ioc": ["libs/packages/ioc/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
