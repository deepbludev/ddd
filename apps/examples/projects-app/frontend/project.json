{
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/examples/projects-app/frontend",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/next:build",
      "outputs": [
        "{options.outputPath}"
      ],
      "defaultConfiguration": "production",
      "options": {
        "root": "apps/examples/projects-app/frontend",
        "outputPath": "dist/apps/examples/projects-app/frontend"
      },
      "configurations": {
        "development": {
          "outputPath": "apps/examples/projects-app/frontend"
        },
        "production": {}
      }
    },
    "serve": {
      "executor": "@nrwl/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "examples-projects-app-frontend:build",
        "dev": true,
        "proxyConfig": "apps/examples/projects-app/frontend/proxy.conf.dev.json"
      },
      "configurations": {
        "development": {
          "buildTarget": "examples-projects-app-frontend:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "examples-projects-app-frontend:build:production",
          "dev": false,
          "proxyConfig": "apps/examples/projects-app/frontend/proxy.conf.json"
        }
      }
    },
    "export": {
      "executor": "@nrwl/next:export",
      "options": {
        "buildTarget": "examples-projects-app-frontend:build:production"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/apps/examples/projects-app/frontend"
      ],
      "options": {
        "jestConfig": "apps/examples/projects-app/frontend/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "apps/examples/projects-app/frontend/**/*.{ts,tsx,js,jsx}"
        ]
      }
    }
  },
  "tags": []
}
